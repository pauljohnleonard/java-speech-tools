package video;

import java.awt.event.KeyAdapter;
import java.awt.event.KeyEvent;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class MakeView {
	
	DrawWC drawWC;

	public MakeView(DrawWC drawWC) {
		this.drawWC = drawWC;
	}

	public void makeWC() {
		JFrame tractFrame = new JFrame();
		tractFrame.add(drawWC);
		tractFrame.setVisible(true);
		tractFrame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		tractFrame.setBounds(0, 0, 640, 520);
	}
}
